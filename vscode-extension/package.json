{
  "name": "sasaklang",
  "displayName": "SasakLang",
  "description": "Syntax highlighting and snippets for SasakLang programming language",
  "version": "1.0.0",
  "publisher": "arjunaayasa",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/arjunaayasa/sasaklang.git"
  },
  "contributes": {
    "languages": [
      {
        "id": "sasaklang",
        "aliases": [
          "SasakLang",
          "sasaklang"
        ],
        "extensions": [
          ".sl",
          ".sasak"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "sasaklang",
        "scopeName": "source.sasaklang",
        "path": "./syntaxes/sasaklang.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "sasaklang",
        "path": "./snippets/sasaklang.json"
      }
    ]
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "keywords": [
    "sasaklang",
    "sasak",
    "lombok",
    "esolang",
    "programming language"
  ],
  "license": "MIT"
}
