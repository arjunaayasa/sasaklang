{
    "Variable Declaration": {
        "prefix": "gawe",
        "body": [
            "gawe ${1:nama} = ${2:nilai}"
        ],
        "description": "Deklarasi variabel dengan gawe"
    },
    "Constant Declaration": {
        "prefix": "tetep",
        "body": [
            "tetep ${1:NAMA} = ${2:nilai}"
        ],
        "description": "Deklarasi konstanta dengan tetep"
    },
    "Print": {
        "prefix": "cetak",
        "body": [
            "cetak(${1:\"pesan\"})"
        ],
        "description": "Cetak output dengan cetak"
    },
    "Input": {
        "prefix": "isik",
        "body": [
            "gawe ${1:input} = isik(${2:\"Prompt: \"})"
        ],
        "description": "Baca input dengan isik"
    },
    "If Statement": {
        "prefix": "lamun",
        "body": [
            "lamun (${1:kondisi}) {",
            "\t${2:// kode}",
            "}"
        ],
        "description": "If statement dengan lamun"
    },
    "If-Else Statement": {
        "prefix": "lamunendah",
        "body": [
            "lamun (${1:kondisi}) {",
            "\t${2:// jika benar}",
            "} endah {",
            "\t${3:// jika salah}",
            "}"
        ],
        "description": "If-else statement dengan lamun/endah"
    },
    "While Loop": {
        "prefix": "selame",
        "body": [
            "selame (${1:kondisi}) {",
            "\t${2:// kode}",
            "}"
        ],
        "description": "While loop dengan selame"
    },
    "For Loop": {
        "prefix": "ojok",
        "body": [
            "ojok (gawe ${1:i} = ${2:0}; ${1:i} < ${3:10}; ${1:i} = ${1:i} + 1) {",
            "\t${4:// kode}",
            "}"
        ],
        "description": "For loop dengan ojok"
    },
    "Function": {
        "prefix": "fungsi",
        "body": [
            "fungsi ${1:nama}(${2:params}) {",
            "\t${3:// kode}",
            "\ttulakan ${4:hasil}",
            "}"
        ],
        "description": "Definisi fungsi dengan fungsi"
    },
    "Return": {
        "prefix": "tulakan",
        "body": [
            "tulakan ${1:nilai}"
        ],
        "description": "Return statement dengan tulakan"
    },
    "Array": {
        "prefix": "daftar",
        "body": [
            "gawe ${1:arr} = [${2:1, 2, 3}]"
        ],
        "description": "Deklarasi array"
    },
    "Map": {
        "prefix": "peta",
        "body": [
            "gawe ${1:obj} = {\"${2:key}\": ${3:value}}"
        ],
        "description": "Deklarasi map/object"
    }
}